<%= f.fields_for :student_address, f.object.build_student_address do |a|%>
  <div class="row">
    <div class="col-12 col-md-6 form-group">
      <%= a.input :country, as: :country, selected: 'ET', priority_countries: ["ET", "US"], input_html: {class: "form-control custom-select"},include_blank: "select country"%>
    </div>
    <div class="col-12 col-md-6 form-group">
      <%= a.input :city, input_html: { class: 'form-control', autocomplete: "city" },placeholder: "City", required: true %>
    </div>
    <div class="col-12 col-md-6 form-group">
      <%= a.input :sub_city, input_html: { class: 'form-control', autocomplete: "sub_city" },placeholder: "Sub city"%>
    </div>
    <div class="col-12 col-md-6 form-group">
      <%= a.input :region, input_html: { class: 'form-control' },placeholder: "Region"%>
    </div>
    <div class="col-12 col-md-6 form-group">
      <%= a.input :zone, input_html: { class: 'form-control', autocomplete: "zone" },placeholder: "Zone"%>
    </div>
    <div class="col-12 col-md-6 form-group">
      <%= a.input :woreda, input_html: { class: 'form-control', autocomplete: "woreda" },placeholder: "Woreda", required: true %>
    </div>
    <div class="col-12 col-md-6 form-group">
      <%= a.input :house_number, input_html: { class: 'form-control', autocomplete: "house_number" },placeholder: "House Number", required: true %>
    </div>
    <div class="col-12 col-md-6 form-group">
      <%= a.input :cell_phone, input_html: { class: 'form-control', 'data-inputmask'=>'"mask": "(999) 999-9999"' }, required: true %>
    </div>
    <div class="col-12 col-md-6 form-group">
      <%= a.input :house_phone, input_html: { class: 'form-control', autocomplete: "house_phone" },placeholder: "House Phone"%>
    </div>
    <div class="col-12 col-md-6 form-group">
      <%= a.input :pobox, input_html: { class: 'form-control', autocomplete: "pobox" },placeholder: "Pobox"%>
    </div>
  </div>
<%end%>